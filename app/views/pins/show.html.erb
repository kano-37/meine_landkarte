<h1>MyMap</h1>
<h3>投稿詳細</h3>
<div class="pin">
  <p><%= @pin.name %></p>
  <p><%= @pin.created_at %></p>
  <% @pin.tags.each do |tag| %>
    <span><%= tag.name %></span>
<% end %>
  <br><%= link_to "編集する", edit_pin_path(@pin.id) %>
  <%= button_to "削除する", pin_path(@pin.id), method: :delete %>
</div>

<%= link_to "Pin一覧に戻る", pins_path %>